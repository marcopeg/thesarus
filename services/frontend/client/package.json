{
    "name": "react-app",
    "version": "0.0.3",
    "description": "Boilerplate for ReactJS project with hot code reloading",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server",
        "start:prod": "cross-env NODE_ENV=production webpack-dev-server",
        "start:styleguide": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.styleguide.js",
        "run:dev": "yarn install && yarn start",
        "run:prod": "yarn install && yarn serve",
        "run:styleguide": "yarn install && yarn start:styleguide",
        "build": "rimraf dist && cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
        "serve": "yarn build && ./node_modules/.bin/http-server ./dist --fallback index.html",
        "make:build": "yarn lint && yarn flow && yarn build",
        "flow": "./node_modules/.bin/flow",
        "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx src"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gaearon/react-hot-boilerplate.git"
    },
    "keywords": [
        "react",
        "reactjs",
        "boilerplate",
        "hot",
        "reload",
        "hmr",
        "live",
        "edit",
        "webpack"
    ],
    "author": "Marco Pegoraro <marco.pegoraro@gmail.com> (https://marcopeg.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
    },
    "homepage": "https://github.com/gaearon/react-hot-boilerplate",
    "devDependencies": {
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-loader": "^6.2.10",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.22.0",
        "babel-preset-stage-2": "^6.22.0",
        "bootstrap-styl": "^5.0.8",
        "copyfiles": "^1.2.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.28.4",
        "custom-hash": "^0.9.0",
        "empty-loader": "^1.0.1",
        "eslint-import-resolver-webpack": "^0.8.3",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.2",
        "flow-bin": "^0.50.0",
        "glob": "^7.1.2",
        "html-webpack-plugin": "^2.29.0",
        "http-server": "^0.10.0",
        "image-webpack-loader": "^3.3.1",
        "optimize-css-assets-webpack-plugin": "^2.0.0",
        "postcss-loader": "^2.0.6",
        "poststylus": "^0.2.3",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-svg-loader": "^1.1.1",
        "rimraf": "^2.6.0",
        "rucksack-css": "^0.9.1",
        "style-loader": "^0.18.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "url-loader": "^0.5.9",
        "webpack": "^2.2.0",
        "webpack-dev-server": "^2.2.0"
    },
    "dependencies": {
        "async": "^2.6.0",
        "babel-eslint": "^7.1.1",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.2",
        "eslint-config-airbnb-base": "^11.2.0",
        "eslint-config-airbnb-flow": "^1.0.2",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.1.0",
        "eslint-plugin-react": "^7.1.0",
        "fastclick": "^1.0.6",
        "history": "^4.6.3",
        "material-ui": "^0.18.6",
        "path-to-regexp": "^1.7.0",
        "prop-types": "^15.5.10",
        "react": "^15.4.2",
        "react-beautiful-dnd": "^2.5.0",
        "react-bootstrap": "^0.31.1",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.2",
        "react-router-dom": "^4.1.2",
        "react-router-prop-types": "^0.0.2",
        "react-router-redux": "next",
        "react-tap-event-plugin": "^2.0.1",
        "redux": "^3.7.2",
        "redux-injector": "^0.1.0",
        "redux-sagas-injector": "^0.1.0",
        "redux-thunk": "^2.2.0",
        "route-parser": "^0.0.5",
        "superagent": "^3.8.1"
    }
}
